{
  "name": "@blynx/test",
  "version": "0.0.4",
  "description": "A library for testing javascript using decorators",
  "main": "./lib/main/index.js",
  "types": "./lib/main/index.d.ts",
  "es2015": "./lib/es2015/index.js",
  "esm2015": "./lib/esm2015/index.js",
  "scripts": {
    "prepare": "tsc && tsc -p tsconfig.es2015.json && tsc -p tsconfig.esm2015.json",
    "test:main": "node lib/main/tests/index.test.js",
    "test:es2015": "node lib/es2015/tests/index.test.js",
    "test": "npm run test:main && npm run test:es2015"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpamp/blynx.git"
  },
  "keywords": [
    "test",
    "decorator",
    "testing",
    "@TestClass",
    "@TestMethod"
  ],
  "author": "Clint Pamperin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cpamp/blynx/issues"
  },
  "homepage": "https://github.com/cpamp/blynx/tree/master/packages/test#readme",
  "dependencies": {
    "colorful-text": "0.0.1",
    "es6-symbol": "^3.1.1",
    "reflect-metadata": "^0.1.10"
  }
}
